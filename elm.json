        "elm/core": "1.0.5 <= v < 2.0.0",
        "elm/html": "1.0.0 <= v < 2.0.0",
        "jfmengels/elm-review": "1.0.0 <= v < 2.0.0",
        "stil4m/elm-syntax": "7.1.1 <= v < 8.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.2.2 <= v < 2.0.0"
    }
}
